[Unit]
Description=API for Revue Cinema
After=syslog.target
After=network.target

[Service]
Type=simple
# Using - in front of path to environment file makes that file *optional*
EnvironmentFile=-/etc/api-revue/environment
ExecStart=/usr/bin/java -jar /opt/revuecinema-0.0.1-SNAPSHOT-standalone.jar

[Install]
WantedBy=multi-user.target
